SELECT I.ISSUESLIST_ID, L.NAME
FROM ISSUES I
JOIN ISSUESLISTS L ON I.ISSUESLIST_ID = L.NAME;

SELECT I.ISSUESLIST_ID, U.ID
FROM ISSUES I
JOIN USERS U ON I.ISSUESLIST_ID = U.ID;

SELECT I.ISSUESLIST_ID, U.FIRSTNAME, U.LASTNAME, COUNT(*) AS ISSUES_NUMBER
FROM USERS U JOIN ISSUES I ON U.ID = I.ISSUESLIST_ID
GROUP BY I.ISSUESLIST_ID;